{
  "manifest_version": 3,
  "name": "To Do List",
  "version": "1.0",
  "description": "A simple to-do list extension",
  "permissions": ["storage"],
  "icons": {
    "48": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
